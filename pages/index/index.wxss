.banners{
  width: 100%;
  height: 300rps;
}

.banners image{
  width: 100%;
  height: 100%;
}

.navContainer{
  display: flex;
  align-items: center;
}

.navContainer .navItem{
  display: flex;
  flex: 1;
  flex-direction: column;
  align-items: center;
}

.navItem .iconfont{
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 90rpx;
  background: #d43c33;
  font-size: 50rpx;
  color: #ffffff;
  margin: 20rpx;
}

.navItem text{
  font-size: 25rpx;
}
